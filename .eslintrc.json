{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-case-declarations": "off",
        "consistent-return": "off",
        "default-case": "off",
        "camelcase": "off"
    }
}
