{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base",
      "plugin:react/recommended"
    //   "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 12,
      "ecmaFeatures": {
          "jsx": true,
          "modules": true
      }
  },
  "rules": {
      "no-case-declarations": "off",
      "consistent-return": "off",
      "default-case": "off",
      "camelcase": "off",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "no-return-assign":"off"
  },
  "settings": {
      "import/resolver": {
          "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
  }
}}